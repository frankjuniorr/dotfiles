visibility: public
namespace: code
tags: [code]
executables:

  - verb: run
    name: python-menu
    description: |
      My Python commands.
      commands avaliable:
        - "Enable venv" : Enable Python virtual environment 
        - "Disable venv" : Disable Python virtual environment 
        - "Shell" : Enter inside of python shell 
        - "Zen" : Show The Zen of Python 
    exec:
      logMode: text
      file: python.sh


  - verb: run
    name: docker-menu
    description: |
      My Docker commands.
      It shows docker custom commands.
      commands avaliable:
        - "ps" : show `docker ps` whith colunm order custom
        - "shell" : Enter inside of some docker container
        - "ephemeral" : start a ephemeral docker container
        - "logs" : show logs for some docker container 
        - "destroy" : destroy the docker environment 
    exec:
      logMode: text
      file: docker.sh
